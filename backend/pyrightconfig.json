{
  "venvPath": ".",
  "venv": "venv",
  "pythonVersion": "3.11",
  "include": [
    "**/*.py"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.*"
  ],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": "none",
  "pythonPlatform": "Windows",
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.11",
      "pythonPlatform": "Windows",
      "extraPaths": [
        "./venv/Lib/site-packages"
      ]
    }
  ]
} 